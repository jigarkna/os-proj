#include<stdio.h>
#include<conio.h>
int main()
{
  int pro,res,k=0,p=0,q=0;
	printf("Enter Process Count : ");
	scanf("%d",&pro);
  printf("\nEnter Resources Count : ");
  scanf("%d",&res);
  int av[res],max[pro][res],given[pro][res],need[pro][res],done[pro];
  for(int i=0;i<pro;i++)
  {
    done[i]=0;
  }
}
